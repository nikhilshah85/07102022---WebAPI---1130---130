



<script>
    function getPostData()
    {
        fetch('https://jsonplaceholder.typicode.com/posts').then( response => response.json())
                                                           .then( (data) =>{
                                                               alert('Call Successfull');
                                                               console.log(data);

                                                               for(var i=0;i<data.length;i++)
                                                               {
                                                                   $("table").append("<tr>" + 
                                                                   
                                                                    "<td>" + data[i].id + "</td>" +
                                                                    "<td>" + data[i].userId + "</td>" +
                                                                    "<td>" + data[i].title + "</td>" +
                                                                    "<td>" + data[i].body + "</td>" +                                                                  
                                                                   
                                                                   + "</tr>")
                                                               }


                                                           })
                                                           .catch( (err) =>{
                                                               alert('Errrorrr');
                                                           })
    }
</script>


<button class="btn btn-primary" onClick="getPostData()"> Show Post Data </button>
<table class="table"></table>